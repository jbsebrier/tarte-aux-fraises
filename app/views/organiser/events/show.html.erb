<h1>Evenement</h1>
<div class="container">
   <div class="row">
     <div class="col-xs-12 col-sm-6 col-sm-offset-3">
        <p>Date: <%= @event.date %></p>
        <p>Lieu <%= @event.city%></p>
        <p>Description <%= @event.description %> </p>
        <%= link_to("Supprimer", organiser_event_path(@event), method: :delete) %>
        <%= link_to("Choix des participants", organiser_event_couples_path(@event)) %>
        <%= link_to("Detail de l'evenement") %>
        <%= link_to("Editer votre evenement", edit_organiser_event_path(@event)) %>

      </div>
  </div>
</div>
